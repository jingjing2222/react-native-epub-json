{
  "name": "react-native-epub-json",
  "root": "packages/react-native-epub-json",
  "projectType": "library",
  "sourceRoot": "packages/react-native-epub-json/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/react-native-epub-json && yarn && yarn build"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/react-native-epub-json && node test.js"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf packages/react-native-epub-json/dist"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/react-native-epub-json && tsc --noEmit"
      }
    }
  }
}