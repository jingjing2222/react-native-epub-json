{
  "name": "react-native-epub-json-monorepo",
  "version": "0.1.0",
  "description": "EPUB to React Native converter monorepo with WASM build tools",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:wasm": "nx build react-native-epub-json-rust",
    "build:ts": "nx build react-native-epub-json",
    "test": "nx test react-native-epub-json",
    "clean": "nx run-many --target=clean --all",
    "dev": "nx serve react-native-epub-json-rust",

    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "nx run-many --target=typecheck --all"
  },
  "keywords": [
    "epub",
    "react-native",
    "wasm",
    "converter",
    "ebook",
    "parser",
    "monorepo"
  ],
  "author": "jingjing2222",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "nx": "^21.3.11"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
